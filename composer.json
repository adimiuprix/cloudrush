{
    "name": "adimiuprix/foren-core",
    "description": "Cloud mining for mining any coin",
    "license": "MIT",
    "type": "project",
    "homepage": "https://myhomepage.com",
    "require": {
        "php": "^8.1",
        "codeigniter4/framework": "^4.0",
        "guzzlehttp/guzzle": "^7.9",
        "pragmarx/random": "^0.2.2",
        "takuya/php-random-string": "^0.1.0"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9",
        "mikey179/vfsstream": "^1.6",
        "phpunit/phpunit": "^10.5.16"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "exclude-from-classmap": [
            "**/Database/Migrations/**"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Support\\": "tests/_support"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "scripts": {
        "test": "phpunit"
    }
}
